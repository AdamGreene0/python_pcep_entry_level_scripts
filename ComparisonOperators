#Program Purpose: Print comparison operator computations

small = 6
medium = 49
large = 777


print(small < medium)#Less than
print(small > large)#Greater than

print()#space


print(small <= small)#Less than or equal to
print(small >= large)#Greater than or equal to


print()#space


print(medium == medium)#equal to
print(medium != medium)#not equal to
