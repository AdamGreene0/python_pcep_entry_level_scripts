#Function with the return condition
def superbowl_hopes(still_winning = True):
    print()
    print("season")
    print("playoffs")
    if not still_winning:
        return
    
    print("Going to the super bowl")

#~~~~Testing~~~~~~~~
superbowl_hopes()
superbowl_hopes(False)
 
